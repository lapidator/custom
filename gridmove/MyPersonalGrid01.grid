  TriggerTop		= [MonitorReal2Bottom]	-	[MonitorReal2Height]/2
  TriggerBottom		= [MonitorReal2Bottom]	-	[MonitorReal2Height]	*0.375
  TriggerLeft		= [MonitorReal2Left]	+	[MonitorReal2Width]		*0.375
  TriggerRight		= [MonitorReal2Right]	-	[MonitorReal2Width]		*0.375

  GridTop			= [Monitor2Bottom]	-	[Monitor2Height]/2	+	8
