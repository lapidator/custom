[Groups]

  NumberOfGroups	= 16

[1]
  ; music player, fancy
  TriggerTop		= [MonitorReal1Bottom]	-	100
  TriggerBottom		= [MonitorReal1Bottom]
  TriggerLeft		= [MonitorReal1Left]	+	[MonitorReal1Width]/2
  TriggerRight		= [MonitorReal1Right]	-	[MonitorReal1Width]/3

  GridTop			= [Monitor1Bottom]	-	115
  GridBottom		= [Monitor1Bottom]	-	15
  GridLeft			= [Monitor1Left]	+	15
  GridRight			= [Monitor1Right]	-	15

[2]
  ; music player
  TriggerTop		= [MonitorReal1Bottom]	-	100
  TriggerBottom		= [MonitorReal1Bottom]
  TriggerLeft		= [MonitorReal1Left]	+	[MonitorReal1Width]/3
  TriggerRight		= [MonitorReal1Right]	-	[MonitorReal1Width]/2

  GridTop			= [Monitor1Bottom]	-	100
  GridBottom		= [Monitor1Bottom]
  GridLeft			= [Monitor1Left]
  GridRight			= [Monitor1Right]

[3]
  ; contacts, fancy
  TriggerTop		= [MonitorReal1Top]		+	100
  TriggerBottom		= [MonitorReal1Top]		+	200
  TriggerLeft		= [MonitorReal1Right]	-	100
  TriggerRight		= [MonitorReal1Right]

  GridTop			= [Monitor1Top]		+	20
  GridBottom		= [Monitor1Top]		+	222
  GridLeft			= [Monitor1Right]	-	166
  GridRight			= [Monitor1Right]	-	15

[4]
  ; contacts
  TriggerTop		= [MonitorReal1Top]
  TriggerBottom		= [MonitorReal1Top]		+	100
  TriggerLeft		= [MonitorReal1Right]	-	100
  TriggerRight		= [MonitorReal1Right]

  GridTop			= [Monitor1Top]		+	16
  GridBottom		= [Monitor1Top]		+	218
  GridLeft			= [Monitor1Right]	-	151
  GridRight			= [Monitor1Right]

[5]
  ; chat
  TriggerTop		= [MonitorReal1Bottom]	-	100
  TriggerBottom		= [MonitorReal1Bottom]
  TriggerLeft		= [MonitorReal1Right]	-	100
  TriggerRight		= [MonitorReal1Right]

  GridTop			= [Monitor1Bottom]	-	510
  GridBottom		= [Monitor1Bottom]	-	130
  GridLeft			= [Monitor1Right]	-	455
  GridRight			= [Monitor1Right]	-	15

[6]
  ; small browser
  TriggerTop		= [MonitorReal1Top]		+	[MonitorReal1Height]	*0.375
  TriggerBottom		= [MonitorReal1Bottom]	-	[MonitorReal1Height]	*0.375
  TriggerLeft		= [MonitorReal1Left]	+	[MonitorReal1Width]		*0.375
  TriggerRight		= [MonitorReal1Right]	-	[MonitorReal1Width]		*0.375

  GridTop			= [Monitor1Top]		+	20
  GridBottom		= [Monitor1Bottom]	-	130
  GridLeft			= [Monitor1Left]	+	15
  GridRight			= [Monitor1Right]	-	470

[7]
  ; big browser
  TriggerTop		= [MonitorReal1Top]		+	[MonitorReal1Height]/4
  TriggerBottom		= [MonitorReal1Bottom]	-	[MonitorReal1Height]/4
  TriggerLeft		= [MonitorReal1Left]	+	[MonitorReal1Width]/4
  TriggerRight		= [MonitorReal1Right]	-	[MonitorReal1Width]/4

  GridTop			= [Monitor1Top]		+	16
  GridBottom		= [Monitor1Bottom]	-	100
  GridLeft			= [Monitor1Left]
  GridRight			= [Monitor1Right]	-	151

[8]
  ; file explorer upper left
  TriggerTop		= [MonitorReal1Top]
  TriggerBottom		= [MonitorReal1Top]		+	[MonitorReal1Height]/2
  TriggerLeft		= [MonitorReal1Left]
  TriggerRight		= [MonitorReal1Left]	+	[MonitorReal1Width]/2

  GridTop			= [Monitor1Top]		+	16
  GridBottom		= [Monitor1Top]		+	[Monitor1Height]/2	+	8
  GridLeft			= [Monitor1Left]
  GridRight			= [Monitor1Left]	+	[Monitor1Width]/2

[9]
  ; file explorer upper right
  TriggerTop		= [MonitorReal1Top]
  TriggerBottom		= [MonitorReal1Top]		+	[MonitorReal1Height]/2
  TriggerLeft		= [MonitorReal1Right]	-	[MonitorReal1Width]/2
  TriggerRight		= [MonitorReal1Right]

  GridTop			= [Monitor1Top]		+	16
  GridBottom		= [Monitor1Top]		+	[Monitor1Height]/2	+	8
  GridLeft			= [Monitor1Right]	-	[Monitor1Width]/2
  GridRight			= [Monitor1Right]

[10]
  ; file explorer lower left
  TriggerTop		= [MonitorReal1Bottom]	-	[MonitorReal1Height]/2
  TriggerBottom		= [MonitorReal1Bottom]
  TriggerLeft		= [MonitorReal1Left]
  TriggerRight		= [MonitorReal1Left]	+	[MonitorReal1Width]/2

  GridTop			= [Monitor1Bottom]	-	[Monitor1Height]/2	+	8
  GridBottom		= [Monitor1Bottom]
  GridLeft			= [Monitor1Left]
  GridRight			= [Monitor1Left]	+	[Monitor1Width]/2

[11]
  ; file explorer lower right
  TriggerTop		= [MonitorReal1Bottom]	-	[MonitorReal1Height]/2
  TriggerBottom		= [MonitorReal1Bottom]
  TriggerLeft		= [MonitorReal1Right]	-	[MonitorReal1Width]/2
  TriggerRight		= [MonitorReal1Right]

  GridTop			= [Monitor1Bottom]	-	[Monitor1Height]/2	+	8
  GridBottom		= [Monitor1Bottom]
  GridLeft			= [Monitor1Right]	-	[Monitor1Width]/2
  GridRight			= [Monitor1Right]

;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~;

